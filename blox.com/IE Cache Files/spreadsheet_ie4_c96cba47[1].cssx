.holder {
	width: 16; height: 16;
	background-image: url( "/alphablox/InfoApps/internal/img/app/smallholder.gif" );
	clip: rect( 0, 16, 16, 0 );
	overflow: hidden;
	cursor: default;
}

.swatch {
	position: absolute;
	top: 3; left: 3;
	width: 10; height: 10;
	clip: rect( 0, 10, 10, 0 );
	overflow: hidden;
	cursor: default;
}

.colorheader {
	font-family: tahoma, helvetica, arial, sans-serif;
	font-size: 8pt;
	width: 124; height: 16;
	background-color: white;
	text-align: center;
	overflow: hidden;
	cursor: default;
}


.border {
	position: absolute;
	top: 0; left: 0;
	width: 24; height: 24;
	background-image: url( "/alphablox/InfoApps/internal/img/app/border.gif" );
	clip: rect( 0, 24, 24, 0 );
	overflow: hidden;
	cursor: default;
}

.inputButton {
	position: absolute;
	font-size: 9pt;
}

.buttonMenu {
	position: absolute;
	height: 24; width: 24;
	clip: rect( 0, 24, 24, 0 );
	background-image: url( "/alphablox/InfoApps/internal/img/app/button9.gif" );
}

.colorSpot {
	position: absolute;
	top: 12px;
	left: 11px;
	width: 9px;
	height: 9px;
	overflow: hidden;
	border: 1 solid black;
}

